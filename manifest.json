{
  "name": "Turtle dialer",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "trigger a call with an ip phone",
  "homepage_url": "http://www.velasuci.com",
  "icons": {
    "16": "icons/turtle16.png",
    "48": "icons/turtle48.png",
    "128": "icons/turtle128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      //"/src/options_custom/lib/store.js",
      "js/phoneformat.js",
      "js/phones.js",
      "js/background.js"
    ],
    //"page": "src/bg/background.html",
    "persistent": true
  },
  "options_page": "html/settings.html",
  "browser_action": {
    "default_icon": "icons/turtle19.png",
    "default_title": "Turtle dialer",
    "default_popup": "html/phonebook.html"
  },
  "permissions": [
    "notifications",
    "storage",
    "contextMenus",
    "http://*/*",
    "https://*/*"
   ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["js/content.js"]
    }
  ]
}